<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="BlissRom-R"
          fetch="https://github.com/BlissRom-R"
          revision="r" />

  <remote name="BlissRoms-Devices"
          fetch="https://github.com/BlissRoms-Devices" />

  <project path="build/make" name="platform_build_make" remote="BlissRom-R" >
  <copyfile src="core/root.mk" dest="Makefile" />
  <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
  <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
  <linkfile src="core" dest="build/core" />
  <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
  <linkfile src="target" dest="build/target" />
  <linkfile src="tools" dest="build/tools" />
  </project>

  <project path="frameworks/av" name="platform_frameworks_av" remote="BlissRom-R" />
  <project path="frameworks/base" name="platform_frameworks_base" remote="BlissRom-R" />
  <project path="frameworks/native" name="platform_frameworks_native" remote="BlissRom-R" />
  <project path="frameworks/opt/telephony" name="platform_frameworks_opt_telephony" remote="BlissRom-R" />
  <project path="frameworks/opt/net/wifi" name="platform_frameworks_opt_net_wifi" remote="BlissRom-R" />

  <project path="packages/apps/Settings" name="platform_packages_apps_Settings" remote="BlissRom-R" />

  <project path="vendor/bliss" name="platform_vendor_bliss" remote="BlissRom-R" />
  <project path="system/core" name="platform_system_core" remote="BlissRom-R" />

</manifest>
